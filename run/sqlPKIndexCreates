alter table TUPLEwarehouse add constraint pk_warehouse 
  primary key (w_id);  

alter table TUPLEdistrict add constraint pk_district 
  primary key (d_w_id, d_id);


alter table TUPLEcustomer add constraint pk_customer 
  primary key (c_w_id, c_d_id, c_id);


alter table TUPLEhistory add constraint pk_history 
 primary key (h_date, h_c_id);

alter table TUPLEoorder add constraint pk_oorder 
  primary key (o_w_id, o_d_id, o_id);

 
alter table TUPLEnew_order add constraint pk_new_order 
  primary key (no_w_id, no_d_id, no_o_id);


alter table TUPLEorder_line add constraint pk_order_line 
  primary key (ol_w_id, ol_d_id, ol_o_id, ol_number);


alter table TUPLEstock add constraint pk_stock
  primary key (s_w_id, s_i_id);


alter table TUPLEitem add constraint pk_item
  primary key (i_id);

 
